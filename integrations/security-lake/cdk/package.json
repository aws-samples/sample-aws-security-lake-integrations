{
  "name": "security-lake-integration-framework",
  "version": "2.0.0",
  "description": "Modular AWS CDK framework for Security Lake integrations from multiple cloud providers",
  "author": "AWS Security Team",
  "license": "SEE LICENSE IN LICENSE",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy -c configFile=config.dev.yaml",
    "deploy:prod": "cdk deploy -c configFile=config.prod.yaml",
    "synth": "cdk synth",
    "synth:dev": "cdk synth -c configFile=config.dev.yaml",
    "diff": "cdk diff",
    "destroy": "cdk destroy",
    "bootstrap": "cdk bootstrap",
    "validate-config": "ts-node scripts/validate-config.ts",
    "migrate-config": "ts-node scripts/migrate-config.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.215.0",
    "aws-sdk": "^2.1470.0",
    "constructs": "^10.3.0",
    "yaml": "^2.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-samples/security-lake-integrations"
  },
  "keywords": [
    "aws",
    "cdk",
    "security-lake",
    "ocsf",
    "azure",
    "gcp",
    "multi-cloud",
    "security",
    "integrations"
  ]
}