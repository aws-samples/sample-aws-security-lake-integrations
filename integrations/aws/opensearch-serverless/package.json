{
  "name": "opensearch-serverless-cdk",
  "version": "1.0.0",
  "description": "AWS CDK project for deploying OpenSearch Serverless collection for Security Lake visualization",
  "author": "AWS Security Team",
  "license": "SEE LICENSE IN LICENSE",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy -c configFile=config.yaml",
    "deploy:prod": "cdk deploy -c configFile=config.prod.yaml",
    "synth": "cdk synth",
    "synth:dev": "cdk synth -c configFile=config.yaml",
    "diff": "cdk diff",
    "destroy": "cdk destroy",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.215.0",
    "constructs": "^10.3.0",
    "yaml": "^2.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "opensearch",
    "opensearch-serverless",
    "security-lake",
    "ocsf",
    "visualization"
